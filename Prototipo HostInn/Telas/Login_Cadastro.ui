       <string notr="true">QComboBox {
    border: 1px solid rgba(255, 255, 255, 200);  /* Borda visível */
    border-radius: 13px;
    background-color: transparent;              /* Totalmente transparente */
    padding: 5px 15px;
    color: white;
    font-size: 12px;
    background-clip: padding-box;
}

QComboBox:focus {
    border: 1px solid rgba(255, 255, 255, 255);  /* Borda mais forte no foco */
    background-color: rgba(255, 255, 255, 36);   /* Leve destaque no foco */
}

/* Remove o botão e a seta */
QComboBox::drop-down {
    border: 1one;
    background: transparent;
    width: 0px;  /* Esconde o espaço reservado pra seta */
}

QComboBox::down-arrow {
    width: 0;
    height: 0;
    border: 1one;
}

/* Dropdown com estilo flat e legível */
QComboBox QAbstractItemView {
    background-color: #2A2A2A;              /* Fundo da lista */
    color: white;                           /* Texto das opções */
    border: 1px solid rgba(255, 255, 255, 50);
    border-radius: 8px;
    selection-background-color: #339CFF;    /* Azul no hover/seleção */
    selection-color: black;
}
